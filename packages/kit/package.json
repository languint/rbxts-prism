{
    "name": "@rbxts/prism-kit",
    "description": "UI kit for prism",
    "version": "1.0.0",
    "author": "longuint-rbx",
    "license": "MIT",
    "main": "out/init.lua",
    "types": "out/index.d.ts",
    "files": [
        "out",
        "!**/*.tsbuildinfo",
        "!**/__tests__"
    ],
    "keywords": [
        "roblox",
        "roblox-ts",
        "ui"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/languint/rbxts-prism.git"
    },
    "bugs": {
        "url": "https://github.com/languint/rbxts-prism/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepack": "pnpm build",
        "build": "shx rm -rf out && rbxtsc --verbose",
        "dev": "shx rm -rf out && rbxtsc -w"
    },
    "devDependencies": {
        "@rbxts/compiler-types": "3.0.0-types.0",
        "@rbxts/prism": "workspace:^",
        "@rbxts/types": "^1.0.813",
        "roblox-ts": "3.0.0",
        "shx": "^0.3.4",
        "typescript": "5.5.3"
    },
    "dependencies": {
        "@rbxts/prism": "workspace:^",
        "@rbxts/services": "^1.5.5",
        "@rbxts/set-timeout": "^1.1.2",
        "@rbxts/vide": "^0.5.3"
    },
    "peerDependencies": {
        "@rbxts/prism": "workspace:^"
    }
}
